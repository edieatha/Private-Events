<h1> Create a new event </h1>

<%= form_for @event do |f| %>
    <%= f.label :title %></br>
    <%= f.text_field :title %></br>
    <%= f.label :description %></br>
    <%= f.text_field :description %></br>
    <%= f.label :date %></br>
    <%= f.date_field :date %></br>
    <%= f.submit %>

<% end %>
