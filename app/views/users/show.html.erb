<%= @user.email%>
<h1>Events Am attending</h1>
<ul>
    <% event = Event.upcoming_events.each do |event| %>
      <li>
      <%= event.even_name%></br>
      <%= event.description %></br>
      <%= event.date%></br>
      </li>
    <% end %>
</ul>

<h1>Events I attended</h1>
<ul>
    <%event = Event.past_events.each do |event| %>
        <li>
        <%= event.even_name%></br>
          <%= event.description %></br>
          <%= event.location %><br>
        <%= event.date%></br>
        </li>
    <% end %>
</ul>

<%= link_to 'Home', events_path %>
